<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="com.brest.ericpol.service.model.impl.EquipmentModelImpl">
		<attributes>
			<id name="equipmentId" />
			<basic name="modification" />
			<basic name="price" />
			<basic name="tankId" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.brest.ericpol.service.model.impl.TankModelImpl">
		<attributes>
			<id name="tankId" />
			<basic name="number">
				<column name="NUMBER_" />
			</basic>
			<basic name="modification" />
			<basic name="price" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
		</attributes>
	</mapped-superclass>
	<entity class="com.brest.ericpol.service.model.impl.EquipmentImpl" name="Equipment">
		<table name="app_Equipment" />
		<attributes />
	</entity>
	<entity class="com.brest.ericpol.service.model.impl.TankImpl" name="Tank">
		<table name="app_Tank" />
		<attributes />
	</entity>
</entity-mappings>