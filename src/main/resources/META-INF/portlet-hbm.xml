<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.brest.ericpol.service.model.Equipment" />
	<import class="com.brest.ericpol.service.model.Tank" />
	<class name="com.brest.ericpol.service.model.impl.EquipmentImpl" table="app_Equipment">
		<cache usage="read-write" />
		<id name="equipmentId" type="long">
			<generator class="assigned" />
		</id>
		<property name="modification" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="price" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="tankId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
	<class name="com.brest.ericpol.service.model.impl.TankImpl" table="app_Tank">
		<cache usage="read-write" />
		<id name="tankId" type="long">
			<generator class="assigned" />
		</id>
		<property name="number" type="com.liferay.portal.dao.orm.hibernate.StringType" column="number_" />
		<property name="modification" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="price" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
</hibernate-mapping>